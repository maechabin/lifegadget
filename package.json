{
  "name": "lifegadget",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/server.jsx",
    "serve": "babel-node ./src/server.jsx",
    "start": "react-scripts start",
    "start_prod": "node ./build/server.jsx",
    "watch": "watchify -t babelify ./src/index.jsx -o ./build/bundle.js",
    "build": "react-scripts build && npm run prod_server && npm run sass",
    "prod": "NODE_ENV=production browserify ./src/index.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/bundle.js",
    "prod_server": "NODE_ENV=production browserify ./src/server.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/server.jsx",
    "prod:win": "set NODE_ENV=production && browserify ./src/index.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/bundle.js",
    "prod_server:win": "set NODE_ENV=production && browserify ./src/server.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/server.jsx",
    "sass": "node-sass ./src/sass/app.scss ./build/style.css --output-style compressed",
    "sass-watch": "node-sass -w ./src/sass/app.scss ./build/style.css --output-style compressed"
  },
  "author": "Lifegadget",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "feed": "^0.3.1",
    "helmet": "^3.4.0",
    "lodash": "^4.17.3",
    "node-fetch": "^1.6.3",
    "object.values": "^1.0.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-scrollchor": "^2.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rss": "^1.2.2",
    "serialize-javascript": "^1.3.0",
    "showdown": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "folderify": "^1.2.1",
    "loose-envify": "^1.3.1",
    "node-sass": "^4.3.0",
    "react-scripts": "^2.1.8",
    "uglify-js": "^2.7.5",
    "watchify": "^3.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
