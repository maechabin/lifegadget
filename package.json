{
  "name": "lifegadget",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "lint": "eslint ./src/server.jsx",
    "serve": "ts-node ./src/server.tsx",
    "start": "react-scripts start",
    "start_prod": "node ./build/server.jsx",
    "watch": "watchify -t babelify ./src/index.tsx -o ./build/bundle.js",
    "build": "react-scripts build && npm run prod_server && npm run sass",
    "prod": "NODE_ENV=production browserify ./src/index.tsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/bundle.js",
    "prod_server": "NODE_ENV=production browserify ./src/server.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/server.jsx",
    "prod:win": "set NODE_ENV=production && browserify ./src/index.tsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/bundle.js",
    "prod_server:win": "set NODE_ENV=production && browserify ./src/server.jsx -t babelify -t loose-envify | uglifyjs -c warnings=false > ./build/server.jsx",
    "sass": "node-sass ./src/sass/app.scss ./build/style.css --output-style compressed",
    "sass-watch": "node-sass -w ./src/sass/app.scss ./build/style.css --output-style compressed"
  },
  "author": "Lifegadget",
  "license": "MIT",
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/lodash": "^4.14.123",
    "@types/node-fetch": "^2.3.2",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/showdown": "^1.9.2",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "feed": "^0.3.1",
    "helmet": "^3.16.0",
    "history": "^4.9.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.3",
    "node-fetch": "^2.3.0",
    "object.values": "^1.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^4.4.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.7",
    "react-scrollchor": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.1.0",
    "rss": "^1.2.2",
    "serialize-javascript": "^1.3.0",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "folderify": "^1.2.1",
    "loose-envify": "^1.3.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.17.0",
    "react-scripts": "^2.1.8",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2",
    "uglify-js": "^2.7.5",
    "watchify": "^3.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
